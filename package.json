{
  "name": "react-micro-list",
  "version": "1.2.2",
  "description": "A Simple React List tool",
  "main": "./lib/index.js",
  "scripts": {
    "start": "webpack --watch --mode development",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -x 'src/test/**' --threshold 90",
    "build": "BABEL_ENV=cjs babel src/ -d lib/",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxshco/react-micro-list.git"
  },
  "keywords": [
    "list",
    "results",
    "react",
    "es6"
  ],
  "author": "Josh Sturrock <jxshco@gmail.com>",
  "bugs": {
    "url": "https://github.com/jxshco/react-micro-list/issues"
  },
  "homepage": "https://github.com/jxshco/react-micro-list#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.82.0",
    "flow-coverage-report": "^0.5.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  }
}
